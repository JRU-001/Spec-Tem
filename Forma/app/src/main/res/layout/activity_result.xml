            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/fechaR" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edadR" app:layout_constraintStart_toStartOf="parent"
